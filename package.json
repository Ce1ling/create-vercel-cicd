{
  "name": "@ce1ling/create-vercel-cicd",
  "version": "1.0.1",
  "description": "GitHub Actions CI/CD for Vercel.",
  "main": "dist/index.js",
  "bin": {
    "create-vercel-cicd": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --skipLibCheck src/index.ts --outDir dist",
