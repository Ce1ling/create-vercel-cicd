  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ce1ling/create-vercel-cicd"
  },
  "bugs": {
    "url": "https://github.com/ce1ling/create-vercel-cicd/issues"
  },
  "author": "Ce1ling",
  "license": "MIT",
  "keywords": [
    "CI/CD",
    "Vercel",
    "Next.js"
  ],
  "devDependencies": {
    "@types/node": "^20",
    "inquirer": "^12.0.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  }
}
